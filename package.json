{
  "name": "animal-crossing-item-db",
  "version": "0.1.0",
  "private": true,
  "license": "CC BY-NC 3.0",
  "dependencies": {
    "@craco/craco": "5.6.4",
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/fs-extra": "8.1.0",
    "@types/got": "9.6.9",
    "@types/jest": "^24.0.0",
    "@types/lodash": "4.14.150",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "5.1.4",
    "@types/react-virtualized-auto-sizer": "1.0.0",
    "babel-plugin-lodash": "3.3.4",
    "framer-motion": "2.0.0-beta.55",
    "fs-extra": "9.0.0",
    "got": "10.7.0",
    "ky": "0.19.0",
    "lodash": "4.17.21",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "2.7.0",
    "react-icons": "3.10.0",
    "react-query": "1.2.1",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "react-virtualized-auto-sizer": "1.0.2",
    "react-window": "1.8.5",
    "slugify": "1.4.0",
    "typescript": "3.8.3",
    "wtf_wikipedia": "8.1.1"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn lint --fix",
    "extract": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/extract/index.ts",
    "extract:watch": "ts-node-dev --compiler-options '{\"module\":\"commonjs\"}' --respawn scripts/extract/index.ts",
    "extract:generate-types": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/extractJsonSchema.ts",
    "extract:validate-types": "jest scripts/validateExtractedDataMatchesSchema.test.ts",
    "load": "ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/load.ts",
    "etl": "yarn extract && yarn extract:generate-types && yarn extract:validate-types && yarn load"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-query": "1.1.2",
    "@types/react-window": "1.8.1",
    "ajv": "6.12.3",
    "bundlewatch": "0.2.6",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "prettier": "2.0.2",
    "source-map-explorer": "2.4.2",
    "ts-jest": "25.3.1",
    "ts-node": "8.8.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript-json-schema": "0.42.0"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "build/static/js/*.js",
        "maxSize": "300kB"
      },
      {
        "path": "build/static/css/*.css",
        "maxSize": "300kB"
      },
      {
        "path": "build/data/*.json",
        "maxSize": "300kB"
      }
    ]
  }
}
